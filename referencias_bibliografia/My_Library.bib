
@article{abu-omarEuropeanAssociationCardioThoracic2017,
  title = {European {{Association}} for {{Cardio}}-{{Thoracic Surgery}} Expert Consensus Statement on the Prevention and Management of Mediastinitis},
  author = {Abu-Omar, Yasir and Kocher, Gregor J. and Bosco, Paolo and Barbero, Cristina and Waller, David and Gudbjartsson, Tomas and Sousa-Uva, Miguel and Licht, Peter B. and Dunning, Joel and Schmid, Ralph A. and Cardillo, Giuseppe},
