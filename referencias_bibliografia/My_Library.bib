  author = {Staplin, Natalie and Herrington, William G. and Judge, Parminder K. and Reith, Christina A. and Haynes, Richard and Landray, Martin J. and Baigent, Colin and Emberson, Jonathan},
  date = {2017-03-07},
  journaltitle = {Clinical Journal of the American Society of Nephrology},
