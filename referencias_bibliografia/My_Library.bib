@article{arguello-casteleiroExploringSemanticDeep2019,
  title = {Exploring Semantic Deep Learning for Building Reliable and Reusable One Health Knowledge from {{PubMed}} Systematic Reviews and Veterinary Clinical Notes},
  author = {Arguello-Casteleiro, Mercedes and Stevens, Robert and Des-Diz, Julio and Wroe, Chris and Fernandez-Prieto, Maria Jesus and Maroto, Nava and Maseda-Fernandez, Diego and Demetriou, George and Peters, Simon and Noble, Peter-John M. and Jones, Phil H. and Dukes-McEwan, Jo and Radford, Alan D. and Keane, John and Nenadic, Goran},
  date = {2019-11-12},
  journaltitle = {Journal of Biomedical Semantics},
  shortjournal = {J Biomed Semantics},
  volume = {10},
  issn = {2041-1480},
  doi = {10.1186/s13326-019-0212-6},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6849172/},
  urldate = {2020-05-31},
  abstract = {Background Deep Learning opens up opportunities for routinely scanning large bodies of biomedical literature and clinical narratives to represent the meaning of biomedical and clinical terms. However, the validation and integration of this knowledge on a scale requires cross checking with ground truths (i.e. evidence-based resources) that are unavailable in an actionable or computable form. In this paper we explore how to turn information about diagnoses, prognoses, therapies and other clinical concepts into computable knowledge using free-text data about human and animal health. We used a Semantic Deep Learning approach that combines the Semantic Web technologies and Deep Learning to acquire and validate knowledge about 11 well-known medical conditions mined from two sets of unstructured free-text data: 300\,K PubMed Systematic Review articles (the PMSB dataset) and 2.5\,M veterinary clinical notes (the VetCN dataset). For each target condition we obtained 20 related clinical concepts using two deep learning methods applied separately on the two datasets, resulting in 880 term pairs (target term, candidate term). Each concept, represented by an n-gram, is mapped to UMLS using MetaMap; we also developed a bespoke method for mapping short forms (e.g. abbreviations and acronyms). Existing ontologies were used to formally represent associations. We also create ontological modules and illustrate how the extracted knowledge can be queried. The evaluation was performed using the content within BMJ Best Practice. Results MetaMap achieves an F measure of 88\% (precision 85\%, recall 91\%) when applied directly to the total of 613 unique candidate terms for the 880 term pairs. When the processing of short forms is included, MetaMap achieves an F measure of 94\% (precision 92\%, recall 96\%). Validation of the term pairs with BMJ Best Practice yields precision between 98 and 99\%. Conclusions The Semantic Deep Learning approach can transform neural embeddings built from unstructured free-text data into reliable and reusable One Health knowledge using ontologies and content from BMJ Best Practice.},
  eprint = {31711540},
  eprinttype = {pmid},
  file = {/home/gusmmm/Zotero/storage/FU2IBWI4/Arguello-Casteleiro et al. - 2019 - Exploring semantic deep learning for building reli.pdf},
  issue = {Suppl 1},
  pmcid = {PMC6849172}
}

@article{arguellocasteleiroCaseStudySepsis2017,
  title = {A {{Case Study}} on {{Sepsis Using PubMed}} and {{Deep Learning}} for {{Ontology Learning}}},
  author = {Arguello Casteleiro, Mercedes and Maseda Fernandez, Diego and Demetriou, George and Read, Warren and Fernandez Prieto, Maria Jesus and Des Diz, Julio and Nenadic, Goran and Keane, John and Stevens, Robert},
  date = {2017},
  journaltitle = {Studies in Health Technology and Informatics},
  shortjournal = {Stud Health Technol Inform},
  volume = {235},
  pages = {516--520},
  issn = {1879-8365},
  abstract = {We investigate the application of distributional semantics models for facilitating unsupervised extraction of biomedical terms from unannotated corpora. Term extraction is used as the first step of an ontology learning process that aims to (semi-)automatic annotation of biomedical concepts and relations from more than 300K PubMed titles and abstracts. We experimented with both traditional distributional semantics methods such as Latent Semantic Analysis (LSA) and Latent Dirichlet Allocation (LDA) as well as the neural language models CBOW and Skip-gram from Deep Learning. The evaluation conducted concentrates on sepsis, a major life-threatening condition, and shows that Deep Learning models outperform LSA and LDA with much higher precision.},
  eprint = {28423846},
  eprinttype = {pmid},
  keywords = {Deep Learning,Humans,Information Storage and Retrieval,Machine Learning,Natural Language Processing,Ontology Learning,OWL,PubMed,Semantics,Sepsis,SPARQL},
  langid = {english}
}

