@article{pisaniDiagnosticAccuracyARDS2019,
  title = {The Diagnostic Accuracy for {{ARDS}} of Global versus Regional Lung Ultrasound Scores - a Post Hoc Analysis of an Observational Study in Invasively Ventilated {{ICU}} Patients},
  author = {Pisani, Luigi and Vercesi, Veronica and van Tongeren, Patricia S. I. and Lagrand, Wim K. and Leopold, Stije J. and Huson, Mischa A. M. and Henwood, Patricia C. and Walden, Andrew and Smit, Marry R. and Riviello, Elisabeth D. and Pelosi, Paolo and Dondorp, Arjen M. and Schultz, Marcus J. and {Lung Ultrasound Consortium}},
  date = {2019-07-25},
  journaltitle = {Intensive Care Medicine Experimental},
  shortjournal = {Intensive Care Med Exp},
  volume = {7},
  pages = {44},
  issn = {2197-425X},
  doi = {10.1186/s40635-019-0241-6},
  abstract = {BACKGROUND: Semi-quantification of lung aeration by ultrasound helps to assess presence and extent of pulmonary pathologies, including the acute respiratory distress syndrome (ARDS). It is uncertain which lung regions add most to the diagnostic accuracy for ARDS of the frequently used global lung ultrasound (LUS) score. We aimed to compare the diagnostic accuracy of the global versus those of regional LUS scores in invasively ventilated intensive care unit patients. METHODS: This was a post-hoc analysis of a single-center observational study in the mixed medical-surgical intensive care unit of a university-affiliated hospital in the Netherlands. Consecutive patients, aged ≥\,18~years, and are expected to receive invasive ventilation for {$>$}\,24~h underwent a LUS examination within the first 2~days of ventilation. The Berlin Definition was used to diagnose ARDS, and to classify ARDS severity. From the 12-region LUS examinations, the global score (minimum 0 to maximum 36) and 3 regional scores (the 'anterior,' 'lateral,' and 'posterior' score, minimum 0 to maximum 12) were computed. The area under the receiver operating characteristic (AUROC) curve was calculated and the best cutoff for ARDS discrimination was determined for all scores. RESULTS: The study enrolled 152 patients; 35 patients had ARDS. The global score was higher in patients with ARDS compared to patients without ARDS (median 19 [15-23] vs. 5 [3-9]; P\,{$<$}\,0.001). The posterior score was the main contributor to the global score, and was the only score that increased significantly with ARDS severity. However, the posterior score performed worse than the global score in diagnosing ARDS, and it had a positive predictive value of only 50 (41-59)\% when using the optimal cutoff. The combined anterolateral score performed as good as the global score (AUROC of 0.91 [0.85-0.97] vs. 0.91 [0.86-0.95]). CONCLUSIONS: While the posterior score increases with ARDS severity, its diagnostic accuracy for ARDS is hampered due to an unfavorable signal-to-noise ratio. An 8-region 'anterolateral' score performs as well as the global score and may prove useful to exclude ARDS in invasively ventilated ICU patients.},
  eprint = {31346914},
  eprinttype = {pmid},
  file = {/home/gusmmm/Zotero/storage/59U8EXRT/Pisani et al. - 2019 - The diagnostic accuracy for ARDS of global versus .pdf},
  issue = {Suppl 1},
  keywords = {ARDS,Diagnosis,Diagnostic accuracy,Intensive care,Lung ultrasound,Ventilation},
  langid = {english},
  options = {useprefix=true},
  pmcid = {PMC6658630}
}

